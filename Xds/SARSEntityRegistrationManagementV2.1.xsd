	<element name="EntityRegistrationManagementRequest"
		type="tns:EntityRegistrationManagementRequestStructure">
	</element>

	<element name="EntityRegistrationManagementResponse"
		type="tns:EntityRegistrationManagementResponseStructure">
	</element>

	<complexType name="EntityRegistrationManagementRequestStructure">
        <sequence>
        	<element name="RequestOperation" maxOccurs="1"
        		minOccurs="1">
        		<simpleType>
        			<restriction base="string">
        				<enumeration value="NEW_REGISTRATION"></enumeration>
        				<enumeration value="MAINTAIN_ENTITY_DETAILS"></enumeration>
        				<enumeration value="CHANGE_FINANCIAL_YEAR_END"></enumeration>
        			</restriction>
        		</simpleType>
        	</element>
        	<element name="ClientCategory" type="bt:IndicatorType"
        		maxOccurs="1" minOccurs="1">
        	</element>
        	<element name="ClientClassification" maxOccurs="1"
        		minOccurs="1">
        		<simpleType>
        			<restriction base="string">
        				<minLength value="1"></minLength>
        				<maxLength value="4"></maxLength>
        			</restriction>
        		</simpleType>
        	</element>
        	<element name="PartyID" type="bt:PartyIDType" maxOccurs="1"
        		minOccurs="1">
        	</element>
        	<element name="BusinessPartnerNo"
        		type="bt:BusinessPartnerNoType" maxOccurs="1" minOccurs="0">
        	</element>
        	<choice>
        		<element name="Individual" maxOccurs="1"
        			minOccurs="1">
        			<complexType>
        				<all>
        					<element name="Surname"
        						type="bt:SurnameType" maxOccurs="1" minOccurs="1">
        					</element>
        					<element name="Initials"
        						type="bt:InitialsType" maxOccurs="1" minOccurs="1">
        					</element>
        					<element name="FirstName"
        						type="bt:FullNamesType" maxOccurs="1" minOccurs="1">
        					</element>
        					<element name="OtherName"
        						type="bt:FullNamesType" maxOccurs="1" minOccurs="0">
        					</element>
        					<element name="DateOfBirth" type="date"
        						maxOccurs="1" minOccurs="1">
        					</element>
        					<element name="MaritalType" maxOccurs="1"
        						minOccurs="1">
        						<simpleType>
        							<restriction base="string">
        								<enumeration value="1"></enumeration>
        								<enumeration value="2"></enumeration>
        								<enumeration value="3"></enumeration>
        								<enumeration value="4"></enumeration>
        								<enumeration value="5"></enumeration>
        								<enumeration value="6"></enumeration>
        							</restriction>
        						</simpleType>
        					</element>
        					<element name="TradingName" type="bt:OrganisationNameType" maxOccurs="1" minOccurs="0"></element>
        				</all>
        			</complexType>
        		</element>
        		<element name="Organisation" maxOccurs="1" minOccurs="1"
        			type="tns:OrganisationStructure">
        		</element>
        	</choice>
        	<element name="ForeignDiplomaticInd" type="bt:YesNoIndType"
        		maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="AllPartnersNaturalPersonInd"
        		type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="ShareBlockInd" type="bt:YesNoIndType"
        		maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="AsylumSeekerInd" type="bt:YesNoIndType"
        		maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="BodyCorporateInd" type="bt:YesNoIndType"
        		maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="EntityStatus" maxOccurs="1" minOccurs="0">
        		<simpleType>
        			<restriction base="string">
        				<minLength value="1"></minLength>
        				<maxLength value="5"></maxLength>
        			</restriction>
        		</simpleType>
        	</element>
        	<element name="CommunicationLanguage" maxOccurs="1"
        		minOccurs="0">
        		<simpleType>
        			<restriction base="string">
        				<minLength value="1"></minLength>
        				<maxLength value="2"></maxLength>
        			</restriction>
        		</simpleType>
        	</element>
        	<element name="CommunicationDetails" maxOccurs="1" minOccurs="1" type="tns:CommunicationDetailsStructure">
              
        	</element>
        	<element name="IdentificationDetails" maxOccurs="1"
        		minOccurs="0" type="tns:IdentificationDetailsStructure">
        	</element>
            <element name="NoLocalBankAccInd" type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        				</element>
            <element name="LocalBankAccInd" type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        				</element>
            <element name="ThirdPartyBankAccInd" type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        				</element>
            <element name="BankAccDeclarationReason" type="tns:BankAccDeclarationReasonStructure" maxOccurs="1" minOccurs="0">
        				</element>
            <element name="BankAccDetails" maxOccurs="1" minOccurs="0">
        		<complexType>
                    <sequence>
        				<element name="BankAccDetail" maxOccurs="9999" minOccurs="1">
        					<complexType>
                                <all>
                                	<element name="ID" type="long"
                                		maxOccurs="1" minOccurs="0">
                                	</element>
                                	<element name="UniqueID"
                                		type="bt:UniqueIdentifierType"
                                		maxOccurs="1" minOccurs="0">
                                	</element>
                                	<element name="BankAccID"
                                		maxOccurs="1" minOccurs="0" type="tns:BankAccIDType">
                                		
                                	</element>
                                	<element name="MarkedForDeletion"
                                		type="bt:YesNoIndType" maxOccurs="1"
                                		minOccurs="0">
                                	</element>
                                    <element name="ConfirmedInd" type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
                                	</element>
                                    <element name="BankAccount"
                                		type="tns:BankAccountStructure"
                                		maxOccurs="1" minOccurs="1">
                                	</element>
                                </all>

        					</complexType>
        				</element>
        			</sequence>

        		</complexType>
        	</element>
        	<element name="RepresentativeDetails"
        		type="tns:RelationshipStructure" maxOccurs="1"
        		minOccurs="0">
        	</element>
        	<element name="OtherRelationships"
        		type="tns:OtherRelationshipsType" maxOccurs="1"
        		minOccurs="0">
        	</element>
        	<element name="MyContactDetails" maxOccurs="1" minOccurs="0">
        		<complexType>
        			<sequence>
        				<element name="MyContactDetail" maxOccurs="9999" minOccurs="1">
        					<complexType>
                                <all>
        							<element name="ID" type="long" maxOccurs="1" minOccurs="0">
        							</element>
        							<element name="UniqueID" type="bt:UniqueIdentifierType" maxOccurs="1" minOccurs="0">
        							</element>
        							<element name="ContactDetaillID" type="tns:ContactDetailIDType" maxOccurs="1" minOccurs="0">
        							</element>
        							<element name="MarkedForDeletion" type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        							</element>
        							<element name="ConfirmedInd" type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        							</element>
        							<element name="Contacts" maxOccurs="1" minOccurs="1">
                                        <complexType>
                                            <all>
                                        		<element name="TelNumbers" maxOccurs="1" minOccurs="0">
                                                    <complexType>
                                                        <sequence>
                                                    		<element name="TelNumber" type="tns:TelNoStructure" maxOccurs="999" minOccurs="1">
                                                    		</element>
                                                    	</sequence>
                                                    </complexType>
                                        		</element>
                                        		<element name="EmailAddresses" maxOccurs="1" minOccurs="0">
                                                    <complexType>
                                                        <sequence>
                                                    		<element name="EmailAddress" type="tns:EmailAddressStructure" maxOccurs="999" minOccurs="1">
                                                    		</element>
                                                    	</sequence>
                                                    </complexType>
                                        		</element>
                                        		<element name="Addresses" maxOccurs="1" minOccurs="0">
                                                    <complexType>
                                                        <sequence>
                                                    		<element name="Address" type="tns:AddressDetailStructure" maxOccurs="999" minOccurs="1">
                                                    		</element>
                                                    	</sequence>
                                                    </complexType>
                                        		</element>
                                        	</all>
                                        </complexType>
        							</element>
        						</all>
        					</complexType></element>
        			</sequence>
        		</complexType></element>
        </sequence>

	</complexType>

	<complexType name="EntityRegistrationManagementResponseStructure">
        <all>
        	<element name="BusinessPartnerNo"
        		type="bt:BusinessPartnerNoType" maxOccurs="1" minOccurs="1">
        	</element>
        	<element name="BusinessPartnerDetails" maxOccurs="1"
        		minOccurs="0">
        		<complexType>
        			<sequence>
        				<element name="BusinessPartnerDetail"
        					maxOccurs="2" minOccurs="1">
        					<complexType>
        						<all>
        							<element name="AccountType"
        								maxOccurs="1" minOccurs="1">
        								<simpleType>
        									<restriction
        										base="string">
        										<enumeration
        											value="MS">
        										</enumeration>
        										<enumeration
        											value="AD">
        										</enumeration>
        									</restriction>
        								</simpleType>
        							</element>
        							<element name="ContractAccountNo"
        								maxOccurs="1" minOccurs="1">
        								<simpleType>
        									<restriction
        										base="string">
        										<minLength value="1"></minLength>
        										<maxLength value="12"></maxLength>
        									</restriction>
        								</simpleType>
        							</element>
        							<element name="ContractObjectNo"
        								maxOccurs="1" minOccurs="1">
        								<simpleType>
        									<restriction
        										base="string">
        										<minLength value="1"></minLength>
        										<maxLength value="20"></maxLength>
        									</restriction>
        								</simpleType>
        							</element>
        							<element name="TaxType"
        								type="bt:TypeOfTaxType" maxOccurs="1" minOccurs="0">
        							</element>
        						</all>

        					</complexType>
        				</element>
        			</sequence>
        		</complexType>
        	</element>
        	<element name="BankDetails" maxOccurs="1" minOccurs="0">
        		<complexType>
        			<sequence>
        				<element name="BankDetail" maxOccurs="999"
        					minOccurs="1">
        					<complexType>
        						<all>
        							<element name="BankAccID"
        								type="tns:BankAccIDType" maxOccurs="1" minOccurs="1">
        							</element>
        							<element name="UniqueID"
        								type="bt:UniqueIdentifierType" maxOccurs="1"
        								minOccurs="0">
        							</element>
        							<element name="ID" type="long"
        								maxOccurs="1" minOccurs="1">
        							</element>
        						</all>
        					</complexType>
        				</element>
        			</sequence>
        		</complexType>
        	</element>
        	<element name="ContactDetails" maxOccurs="1"
        		minOccurs="0">
        		<complexType>
        			<sequence>
        				<element name="ContactDetail" maxOccurs="9999"
        					minOccurs="1">
        					<complexType>
        						<all>
        							<element name="ID" type="long"
        								maxOccurs="1" minOccurs="0">
        							</element>
        							<element name="UniqueID"
        								type="bt:UniqueIdentifierType" maxOccurs="1"
        								minOccurs="0">
        							</element>
        							<element name="ContactDetailID"
        								type="tns:ContactDetailIDType" maxOccurs="1"
        								minOccurs="0">
        							</element>
        							<element name="Contacts"
        								maxOccurs="1" minOccurs="1">
        								<complexType>
        									<all>
        										<element
        											name="TelNumbers" maxOccurs="1" minOccurs="0">
        											<complexType>
        												<sequence>
        													<element
        														name="TelNumber" maxOccurs="999" minOccurs="1">
        														<complexType>
        															<all>
        																<element
        																	name="ID" type="long" maxOccurs="1"
        																	minOccurs="1">
        																</element>
        																<element
        																	name="TelSeqNo" type="tns:TelNoSeqNoType"
        																	maxOccurs="1" minOccurs="0">
        																</element>
        																<element
        																	name="TelNoDetailID" type="tns:ContactDetailIDType" maxOccurs="1" minOccurs="1">
        																</element>
        																<element
        																	name="UniqueID" type="bt:UniqueIdentifierType"
        																	maxOccurs="1" minOccurs="0">
        																</element>
        															</all>
        														</complexType>
        													</element>
        												</sequence>
        											</complexType>
        										</element>
        										<element
        											name="EmailAddresses" maxOccurs="1" minOccurs="0">
        											<complexType>
        												<sequence>
        													<element
        														name="EmailAddress" maxOccurs="999"
        														minOccurs="1">
        														<complexType>
        															<all>
        																<element
        																	name="ID" type="long" maxOccurs="1"
        																	minOccurs="1">
        																</element>
        																<element
        																	name="UniqueID" type="bt:UniqueIdentifierType"
        																	maxOccurs="1" minOccurs="0">
        																</element>
        																<element
        																	name="EmailSeqNo" type="tns:EmailSeqNoType"
        																	maxOccurs="1" minOccurs="0">
        																</element>
        																<element
        																	name="EmailDetailID" type="tns:ContactDetailIDType" maxOccurs="1" minOccurs="1">
        																</element>
        															</all>
        														</complexType>
        													</element>
        												</sequence>
        											</complexType>
        										</element>
        										<element
        											name="Addresses" maxOccurs="1" minOccurs="0">
        											<complexType>
        												<sequence>
        													<element
        														name="Address" maxOccurs="999" minOccurs="1">
        														<complexType>
        															<all>
                                                                        <element name="ID" type="long" maxOccurs="1" minOccurs="1">
        																</element>
                                                                        <element
        																	name="UniqueID" type="bt:UniqueIdentifierType"
        																	maxOccurs="1" minOccurs="0">
        																</element>
        																<element
        																	name="ContactDetailID"
        																	type="tns:ContactDetailIDType" maxOccurs="1"
        																	minOccurs="1">
        																</element>
        															</all>
        														</complexType>
        													</element>
        												</sequence>

        											</complexType>
        										</element>
        									</all>
        								</complexType>
        							</element>
        						</all>

        					</complexType>
        				</element>
        			</sequence>
        		</complexType>
        	</element>
        	<element name="CurrentFYE" type="bt:FinancialYearEndType" maxOccurs="1" minOccurs="0"></element>
        	<element name="NewFYE" type="bt:FinancialYearEndType" maxOccurs="1" minOccurs="0"></element>
            <element name="EffectivePeriodFYE" type="bt:YearType" maxOccurs="1" minOccurs="0"></element>
        </all>
	</complexType>

	<complexType name="ContactDetailStructure">
		<all>
			<element name="TelNoDetails" maxOccurs="1" minOccurs="0">
				<complexType>
					<sequence>
						<element name="TelNoDetail" maxOccurs="4" minOccurs="1" type="tns:TelNoStructure">
							</element>
					</sequence>
				</complexType></element>
			<element name="NoCellPhoneInd" type="bt:YesNoIndType"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="NoEmailInd" type="bt:YesNoIndType"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="Email" type="tns:EmailAddressStructure" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="WebAdress" type="bt:EmailType" maxOccurs="1"
				minOccurs="0">
			</element>

			<element name="AddressDetails" maxOccurs="1"
				minOccurs="0">
				<complexType>
					<sequence>
						<element name="AddressDetail"
							type="tns:AddressDetailStructure" maxOccurs="2" minOccurs="1">
						</element>
					</sequence>
				</complexType>
			</element>
			<element name="Metadata" type="bt:MetadataStructure"
				maxOccurs="1" minOccurs="0">
			</element>
		</all>
	</complexType>

	<complexType name="TelNoStructure">
        <all>
        	<element name="TelNo" type="bt:TelFaxCellNoType"
        		maxOccurs="1" minOccurs="1">
        	</element>
        	<element name="TelNoCountry" type="bt:CountryType"
        		maxOccurs="1" minOccurs="1">
        	</element>
        	<element name="RegisteredNoInd" type="bt:YesNoIndType"
        		maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="ID" type="long" maxOccurs="1"
        		minOccurs="1">
        	</element>
        	<element name="UniqueID" type="bt:UniqueIdentifierType"
        		maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="TelSeqNo" type="tns:TelNoSeqNoType" maxOccurs="1" minOccurs="0"></element>
        	<element name="TelNoDetailID" maxOccurs="1" minOccurs="0"
        		type="tns:ContactDetailIDType">
        	</element>
        	<element name="MarkedForDeletion" type="bt:YesNoIndType"
        		maxOccurs="1" minOccurs="0">
        	</element>
        	<element name="Use" maxOccurs="1" minOccurs="1">
        		<simpleType>
        			<restriction base="string">
        				<enumeration value="HOME"></enumeration>
        				<enumeration value="BUSINESS"></enumeration>
        				<enumeration value="FAX"></enumeration>
        				<enumeration value="CELL"></enumeration>
        			</restriction>
        		</simpleType>
        	</element>
        	<element name="Usages" maxOccurs="1" minOccurs="0"
        		type="tns:UsagesStructure">
        	</element>
        </all>
	</complexType>
	
	<complexType name="UsagesStructure">
		<sequence>
			<element minOccurs="1" maxOccurs="99" name="Usage">
				<complexType>
					<all>
						<element minOccurs="0" maxOccurs="1"
							name="TaxRefDetails">
							<complexType>
								<all>
									<element minOccurs="1" maxOccurs="1"
										name="TaxType" type="bt:TypeOfTaxType">
									</element>
									<element minOccurs="0" maxOccurs="1"
										name="TaxRefNo" type="bt:TaxRefNoType">
									</element>
								</all>
							</complexType>
						</element>
					</all>
				</complexType>
			</element>
		</sequence>
	</complexType>
	

	<complexType name="AddressDetailStructure">
        <choice>
        	<element name="PhysicalAddressDetails" maxOccurs="1"
        		minOccurs="1" type="tns:PhysicalAddressDetailsStructure">
        	</element>
        	<element name="PostalAddressDetails" maxOccurs="1"
        		minOccurs="1">
        		<complexType>
        			<all>
        				<element name="SameAsPhysicalAddressInd"
        					type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        				</element>
        				<element name="CareOfInd" type="bt:YesNoIndType"
        					maxOccurs="1" minOccurs="0">
        				</element>
        				<element name="RegisteredPostalAddressInd"
        					type="bt:YesNoIndType" maxOccurs="1" minOccurs="0">
        				</element>
        				<element name="ID" type="long" maxOccurs="1" minOccurs="1"></element>
        				<element name="UniqueID" type="bt:UniqueIdentifierType" maxOccurs="1" minOccurs="0"></element>
        				<element name="ContactDetaillID"
        					type="tns:ContactDetailIDType" maxOccurs="1" minOccurs="0">
        				</element>
